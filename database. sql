CREATE DATABASE IF NOT EXISTS electricity CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci;
USE electricity;

DROP TABLE IF EXISTS factures;

CREATE TABLE factures (
    id INT AUTO_INCREMENT PRIMARY KEY,
    mois VARCHAR(20),
    annee INT,
    consommation_kwh INT,
    montant_da INT
);

